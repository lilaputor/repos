const randomNumber = Math.floor(Math.random() * 100) + 1;
let guess;
do {
  guess = parseInt(prompt("Угадайте число от 1 до 100:"));
  if (guess > randomNumber) {
    console.log("Меньше!");
  } else if (guess < randomNumber) {
    console.log("Больше!");
  } else {
    console.log("Правильно!");
  }
} while (guess !== randomNumber);
